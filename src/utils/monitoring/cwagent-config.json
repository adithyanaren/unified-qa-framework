{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root",
    "region": "us-east-1"
  },
  "metrics": {
    "metrics_collected": {
      "prometheus": {
        "prometheus_config_path": "/etc/cwagentprometheus/prometheus.yaml"
      }
    },
    "append_dimensions": {},
    "force_flush_interval": 5
  }
}
